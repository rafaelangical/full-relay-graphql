		"@react-native-community/eslint-config": "^0.0.5",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
