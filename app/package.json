{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"android:apk": "cd android && ./gradlew assembleRelease",
		"android:install": "adb install android/app/build/outputs/apk/app-release.apk",
		"clear": "react-native start --reset-cache",
		"devtools": "react-devtools",
		"ios": "react-native run-ios",
		"lint": "eslint src",
		"lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
		"lint:staged": "lint-staged",
		"relay": "relay-compiler --watchman false --src ./src --schema data/schema.graphql --language typescript",
		"relay:watch": "yarn relay -- --watch",
		"start": "react-native start",
		"test": "jest"
	},
	"dependencies": {
		"@react-native-community/async-storage": "^1.6.3",
